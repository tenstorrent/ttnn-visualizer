{
  "name": "ttnn-visualizer",
  "private": true,
  "version": "0.46.0",
  "type": "module",
  "engines": {
    "node": "22.12.0"
  },
  "scripts": {
    "build:wheel": "pnpm run build && python -m build --wheel",
    "build": "vite build",
    "dev:server": "VITE_SERVER_MODE=1 vite",
    "dev": "vite",
    "flask:lint": "isort --check-only backend/ttnn_visualizer && black --check backend/ttnn_visualizer",
    "flask:mypy": "PYTHONPATH=backend mypy backend/ttnn_visualizer",
    "flask:start-debug": "DEBUG=true PYTHONPATH=backend python -m ttnn_visualizer.app",
    "flask:start": "PYTHONPATH=backend python -m ttnn_visualizer.app",
    "flask:test": "PYTHONPATH=backend pytest backend/ttnn_visualizer/tests",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,css,json,md}\"",
    "lint:fix": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:spdx": "node scripts/check-spdx.mjs",
    "lint:licenses": "pnpm licenses ls --json | node ./scripts/check-missing-dep-licenses.mjs",
    "lint:licenses:fix": "pnpm licenses ls --json | node scripts/check-missing-dep-licenses.mjs --fix=true",
    "lint:ts": "npx tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "preview": "vite preview",
    "release:major": "node scripts/release.mjs -major",
    "release:minor": "node scripts/release.mjs -minor",
    "release:patch": "node scripts/release.mjs -patch",
    "release": "node scripts/release.mjs -minor",
    "serve": "vite build && FLASK_ENV=production pnpm run flask:start",
    "test:watch": "vitest",
    "test": "vitest run --environment jsdom"
  },
  "dependencies": {
    "@blueprintjs/colors": "^5.1.8",
    "@blueprintjs/core": "^6.1.0",
    "@blueprintjs/icons": "^6.0.0",
    "@blueprintjs/select": "^6.0.1",
    "@blueprintjs/table": "^6.0.1",
    "@tanstack/react-virtual": "^3.13.12",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "css-select": "^5.2.2",
    "dom-serializer": "^2.0.0",
    "immutable": "^5.1.3",
    "jotai": "^2.13.0",
    "mini-svg-data-uri": "^1.4.4",
    "normalize.css": "^8.0.1",
    "papaparse": "^5.5.3",
    "plotly.js": "^2.35.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-plotly.js": "^2.6.0",
    "react-query": "^3.39.3",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "react-toastify": "^10.0.6",
    "socket.io-client": "^4.8.1",
    "tinycolor2": "^1.6.0",
    "vis-data": "^7.1.10",
    "vis-network": "^9.1.13"
  },
  "devDependencies": {
    "@blueprintjs/node-build-scripts": "^9.2.7",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-plotly.js": "^2.6.3",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.7.0",
    "classnames": "^2.5.1",
    "eslint": "^9.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "highlight.js": "^11.11.1",
    "htmlparser2": "^9.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "only-allow": "^1.2.1",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "stylelint": "^16.23.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^3.3.2",
    "typescript": "~5.2.2",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3"
  },
  "browserslist": [
    "defaults",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint ./src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
    ],
    "**/*.scss": "npx stylelint --fix",
    "**/*.{js,jsx,ts,tsx,css,scss}": "pnpm run format",
    "backend/ttnn_visualizer/**/*.py": "pnpm run flask:lint"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Tenstorrent AI ULC",
    "url": "https://tenstorrent.com/"
  }
}
