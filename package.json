{
  "name": "ttnn-visualizer",
  "private": true,
  "version": "0.72.0",
  "type": "module",
  "description": "A tool for visualizing the Tenstorrent Neural Network model (TT-NN)",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenstorrent/ttnn-visualizer/"
  },
  "author": {
    "name": "Tenstorrent AI ULC",
    "url": "https://tenstorrent.com/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "22.21.0",
    "pnpm": ">=10"
  },
  "scripts": {
    "build:wheel": "pnpm run build && python -m build --wheel",
    "build": "vite build",
    "build:docs": "sphinx-build ./docs ./docs/output",
    "dev:server": "VITE_SERVER_MODE=1 vite",
    "dev": "vite",
    "flask:lint": "isort --check-only backend/ttnn_visualizer && black --check backend/ttnn_visualizer",
    "flask:format": "isort backend/ttnn_visualizer && black backend/ttnn_visualizer",
    "flask:mypy": "PYTHONPATH=backend mypy backend/ttnn_visualizer",
    "flask:start-debug": "DEBUG=true PYTHONPATH=backend python -m ttnn_visualizer.app",
    "flask:start": "PYTHONPATH=backend python -m ttnn_visualizer.app",
    "flask:test": "PYTHONPATH=backend pytest backend/ttnn_visualizer/tests",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,css,json,md}\"",
    "lint:fix": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:spdx": "node scripts/check-spdx.mjs",
    "lint:licenses": "pnpm licenses ls --json | node ./scripts/check-missing-dep-licenses.mjs",
    "lint:licenses:fix": "pnpm licenses ls --json | node scripts/check-missing-dep-licenses.mjs --fix=true",
    "lint:ts": "npx tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "preview": "vite preview",
    "release:major": "node scripts/release.mjs -major",
    "release:minor": "node scripts/release.mjs -minor",
    "release:patch": "node scripts/release.mjs -patch",
    "release": "node scripts/release.mjs -minor",
    "serve": "vite build && FLASK_ENV=production pnpm run flask:start",
    "test:watch": "vitest",
    "test": "vitest run --environment jsdom"
  },
  "dependencies": {
    "@blueprintjs/colors": "^5.1.12",
    "@blueprintjs/core": "^6.6.1",
    "@blueprintjs/icons": "^6.5.0",
    "@blueprintjs/select": "^6.0.13",
    "@blueprintjs/table": "^6.0.13",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "ajv": "^8.17.1",
    "axios": "^1.13.3",
    "jotai": "^2.16.2",
    "normalize.css": "^8.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-plotly.js": "^2.6.0",
    "react-router": "^6.30.3",
    "react-router-dom": "^6.30.3",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.3",
    "tinycolor2": "^1.6.0",
    "vis-data": "^8.0.3",
    "vis-network": "^10.0.2"
  },
  "devDependencies": {
    "@blueprintjs/node-build-scripts": "^9.2.10",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-plotly.js": "^2.6.4",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^4.7.0",
    "classnames": "^2.5.1",
    "eslint": "^9.39.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-compat": "^6.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "highlight.js": "^11.11.1",
    "htmlparser2": "^9.1.0",
    "husky": "^9.1.7",
    "immutable": "^5.1.4",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.5.2",
    "mini-svg-data-uri": "^1.4.4",
    "only-allow": "^1.2.2",
    "prettier": "~3.6.2",
    "sass": "^1.97.3",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^3.3.2",
    "typescript": "~5.2.2",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3"
  },
  "browserslist": [
    "defaults",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix --report-unused-disable-directives --max-warnings 0"
    ],
    "**/*.scss": "npx stylelint --fix",
    "**/*.{js,jsx,ts,tsx,css,scss}": "pnpm run format",
    "backend/ttnn_visualizer/**/*.py": "pnpm run flask:format"
  }
}
